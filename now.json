{
  "version": 2,
  "name": "statickit-site",
  "scope": "statickit",
  "alias": ["statickit.com", "www.statickit.com"],
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "build": {
    "env": {
      "STATICKIT_URL": "https://api.statickit.com",
      "FORM_ID": "0858b1a135b0"
    }
  },
  "routes": [
    {
      "src": "/static/fonts/(.*)",
      "headers": { "cache-control": "public, max-age=31536000" },
      "dest": "/static/fonts/$1"
    }
  ]
}
